{
  "https_port": 443,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:8080"
      },
      "HttpsInlineCertAndKeyFile": {
        "Url": "https://+:8081",
        "Certificate": {
          "Path": "/var/cert/fullchain.pem",
          "KeyPath": "/var/cert/privatekey.pem"
        }
      }
    }
  },
  "AllowedHosts": "*"
}